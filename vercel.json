{
  "version": 2,
  "buildCommand": "npx prisma generate && npx prisma migrate deploy && npm run build",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "public"
      }
    },
    {
      "src": "vercel-func.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "vercel-func.js"
    }
  ]
}
